{
  "name": "@eosdg/questions",
  "private": false,
  "version": "0.4.0",
  "description": "Questions for the Games",
  "main": "lib/cjs/bundle.js",
  "module": "lib/esm/bundle.js",
  "types": "lib/cjs/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "jest",
    "prebuild": "npm run clean",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json && rollup -c rollup.config.js",
    "build:cjs": "tsc -p tsconfig-cjs.json && rollup -c rollup.cjs-config.js",
    "zip": "zip-folder lib",
    "release": "dotenv release-it",
    "clean": "del-cli lib"
    "doc": " npx @adobe/jsonschema2md -d schema -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eosdg/questions.git"
  },
  "author": "Julian Pollinger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eosdg/questions/issues"
  },
  "homepage": "https://github.com/eosdg/questions#readme",
  "devDependencies": {
    "@adobe/jsonschema2md": "^5.0.2",
    "@neferin/zip-folder": "^1.0.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.1.1",
    "del-cli": "^3.0.1",
    "dotenv-cli": "^4.0.0",
    "jest": "^26.6.3",
    "jest-json-schema": "^2.1.0",
    "jest-junit": "^12.0.0",
    "release-it": "^14.2.2",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.5"
  },
  "jest-junit": {
    "suiteName": "Fragen Tests",
    "includeConsoleOutput": true
  }
}
